{
    "manifest_version": 3,
    "name": "YT to MP3/MP4 Downloader",
    "description": "Download YouTube videos as MP3 or MP4 using local server",
    "version": "1.0",
    "permissions": ["activeTab", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  